			/**
			 * 清除号码
			 */
			public function clean():void
			{
				init();
				if(changeType == 5)
				{
					bettingRandomStander.clearAll();
					return;
				}
				if(changeType == 6)
				{
					bettingRandomStick.clearAll();
					return;
				}
				if(changeType == 7)
				{
					bettingRandomDantuo.clearAll();
					return;
				}
				if(changeType == 8)
				{
					unLocationSelected.clearAll();
					return;
				}
				if(changeType == 9)
				{
					locationSelected.clearAll();
					return;
				}
				if(changeType == 10)
				{
					interestPlay.clearAll();
					return;
				}
				if(changeType == 11)
				{
					ForwardOne_Content.init();
					return;
				}
				if(game_ID == "01"){
					RandomOne_Content.init();
					RandomOne_Content.TipText = resourceManager.getString('LotteryTips','ssc_game_tip_emp_49');
				}
				if(game_ID == "02" || game_ID == "03" || game_ID == "04" || game_ID == "05" || game_ID == "06" || game_ID == "07" || game_ID == "08")
				{
					RandomTwo_Content.clean();
				}
				if(game_ID == "09" || game_ID == "10"){ForwardTwo_Content.clean();}
				if(game_ID == "11" || game_ID == "12"){ForwardThree_Content.clean();}
			}
