					}
					else{
						toggleBtn.styleName = "menuSelectSkinStyle";
					}
					if(i == 1){
						toggleBtn.label = resourceManager.getString('LotteryTips','lottery_12_Random1');
						toggleBtn.visible = false; toggleBtn.includeInLayout = false;
					}
					if(i == 2){toggleBtn.label = resourceManager.getString('LotteryTips','lottery_12_Random2');	toggleBtn.visible = false; toggleBtn.includeInLayout = false;}
					if(i == 3){toggleBtn.label = resourceManager.getString('LotteryTips','lottery_12_Random3');	toggleBtn.visible = false; toggleBtn.includeInLayout = false;}
